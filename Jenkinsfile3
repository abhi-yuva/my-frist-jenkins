pipeline{
    agent any

    stages{
        stage('Connect to Git'){
            steps{
                sh 'checkout scmGit(branches: [[name: '*/master']], extensions: [], userRemoteConfigs: [[credentialsId: 'abhi-yuva', url: 'https://github.com/abhi-yuva/my-frist-jenkins.git']])'
            }
        }

        stage('Second Stage'){
            steps{
                echo "This is my Second Stage"

            }
        }

        stage('Third Stage'){
            steps{
                echo "this is my third STage"
            }

        }
    }
}
