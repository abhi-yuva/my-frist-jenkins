pipeline{
    agent any

    stages{
        stage('First Stage'){
            steps{
                echo "This is my First Stage"
                echo "Welcome to Declrative Pipeline"
            }
        }

        stage('Second Stage'){
            steps{
                echo "This is my Second Stage"

            }
        }

        stage('Third Stage'){
            steps{
                echo "this is my third STage"
            }

        }
    }
}