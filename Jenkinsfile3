pipeline{
    agent any

    stages{
        stage('Connect to Git'){
            steps{
                git url: 'https://github.com/abhi-yuva/maven.git' , branch:'main', 
                credentialsId: 'abhi-yuva'

                }
        }

        stage('Second Stage'){
            steps{
                echo "This is my Second Stage"

            }
        }

        stage('Third Stage'){
            steps{
                echo "this is my third STage"
            }

        }
    }
}
