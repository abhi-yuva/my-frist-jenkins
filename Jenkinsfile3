pipeline{
    agent any

    tools {
        maven "MAVEN"
    }

    stages{
        stage('Connect to Git'){
            steps{
                git url: 'https://github.com/abhi-yuva/maven.git' , branch:'main', 
                credentialsId: 'abhi-yuva'

                }
        }

        stage('Second Stage'){
            steps{
                sh 'mvn package'

            }
        }

        stage('Third Stage'){
            steps{
                sh 'mvn ttest'
            }

        }
    }
}
